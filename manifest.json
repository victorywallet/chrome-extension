{
  "manifest_version": 3,
  "name": "Victory Wallet",
  "version": "1.8",
  "key": "",
  "description": "An other crypto wallet ",
  "icons": {
    "128": "icons/victory2on.png"
  },
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "128": "icons/victory2.png"
    }
  },
  "side_panel": {
    "default_path": "html/popup.html"
  },

  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "sidePanel",
    "notifications",
    "unlimitedStorage"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/inject.js"
      ],
      "run_at": "document_start",
      "world": "ISOLATED"
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "dist/provider.js",
        "dist/select.js",
        "html/select.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],

  "host_permissions": [
    "*://connect.trezor.io/9/*"
  ],

  "externally_connectable": {
    "matches": ["<all_urls>"]
  },

  "background": {
    "service_worker": "dist/service.js"
  }
}